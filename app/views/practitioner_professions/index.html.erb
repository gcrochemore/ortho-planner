<p id="notice"><%= notice %></p>

<h1>Listing Practitioner Professions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @practitioner_professions.each do |practitioner_profession| %>
      <tr>
        <td><%= practitioner_profession.name %></td>
        <td><%= link_to 'Show', practitioner_profession %></td>
        <td><%= link_to 'Edit', edit_practitioner_profession_path(practitioner_profession) %></td>
        <td><%= link_to 'Destroy', practitioner_profession, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Practitioner profession', new_practitioner_profession_path %>
